[sharedDocuments]

path = /fileserver/sharedDocuments
force user = fileshare
force group = fileshareuser
create mask = 0664
force create mode = 0664
directory mask = 0777
force directory mode = 0777
public = yes
writable = yes

[sharedMedia]

path = /fileserver/sharedMedia
force user = fileshare
force group = fileshareuser
create mask = 0664
force create mode = 0664
directory mask = 0777
force directory mode = 0777
public = yes
writable = yes
